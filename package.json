{
  "name": "repeating-form-backend",
  "version": "1.0.0",
  "description": "Backend server for handling repeating form submissions",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "validate:env": "node config/env-validator.js",
    "build": "node build.js",
    "build:prod": "NODE_ENV=production node build.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/services tests/errors",
    "test:integration": "jest tests/integration",
